{
    "name": "Coding Interview Backend Level 3",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "remoteUser": "node",
    "forwardPorts": [3000],
    "postCreateCommand": "npm ci",
    "shutdownAction": "none",
    "mounts": [
        "source=${localWorkspaceFolder}/.npm-cache,target=/home/node/.npm-cache,type=bind"
    ]
}
